<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/transparent_8percent" />
            <stroke android:width="1dp" android:color="@color/color_bg_button_continue" />
            <corners android:radius="16dp" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/color_bg_edit_text" />
            <stroke android:width="1dp" android:color="@color/border_edt_pin" />
            <corners android:radius="16dp" />
        </shape>
    </item>
</selector>
